generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model savedSearches {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  city     String
  dateTime String
  feel     String
  high     String
  humidity String
  lat      String
  long     String
  low      String
  sunrise  String
  temp     String
}
